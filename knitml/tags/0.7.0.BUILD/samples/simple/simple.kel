Pattern 'en' {  GeneralInformation {    Name: 'Simple Square'    Description: 'A simple square'    Dimensions: '4 in by 4 in'    Gauge 'stockinette' {      StitchGauge: 6 stitchesPerInch      RowGauge: 6 rowsPerInch    }  }  Supplies {    Yarns {      YarnType 'the-yarn-type' [weight:'worsted']    }    Needles {      NeedleType 'size-4' circular { size: 4 US }      Needle 'the-needle' [typeref:'size-4']    }    Accessories  }  Directions {    CastOn 24    Instruction 'stockinette-st' {      Row: k to end      Row: p to end    }    Repeat 'stockinette-st' until measures 4 in    NextRow: BindOff all sts  }}